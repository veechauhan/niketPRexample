GET     /                           controllers.HomeController.index
GET     /signIn                     controllers.HomeController.signingIn(authToken: String)
GET     /contributions/:pageId      controllers.HomeController.contributions(pageId: Int)
GET     /signUp                     controllers.HomeController.signUp
GET     /notifications              controllers.HomeController.notifications
GET     /profile                    controllers.HomeController.profile
GET     /studioUsers/:pageId        controllers.ManagerController.studioUsers(pageId)
GET     /addContribution            controllers.HomeController.addContribution(contributionName:String,contributionUrl:String,contributionType:String,status:String,remark:String)



GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
